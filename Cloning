import cv2
image = cv2.imread('sq.png")
num_clones = 5
offset = 50
width= image.shape[1] + (num_clones - 1) offset
height=image.shape[0]
new_image = np.zeros((height, width, 3), dtype=np.uint8)
new_image[0:image.shape[0], 0:image.shape[1]] = image
for i in range(1, num_clones):
x_offset=1*offset
y_offset = 0
new_image[y_offset:y_offset + image.shape[0], x_offset:x_offset + image.shape[1]] = image
cv2_imshow(new_image)
